# Ron Clanker Environment Configuration
# Copy this file to .env and fill in your actual values
# NEVER commit .env to git!

# Database Configuration
DB_PASSWORD=your_secure_password_here

# Redis Configuration (usually defaults are fine)
REDIS_URL=redis://redis:6379

# Postgres Configuration
POSTGRES_URL=postgresql://ron:${DB_PASSWORD}@postgres:5432/ron_clanker

# FPL API
FPL_API_URL=https://fantasy.premierleague.com/api

# Your FPL Team (Register at fantasy.premierleague.com first)
FPL_TEAM_ID=your_team_id_here
FPL_TEAM_NAME=Your Team Name

# FPL Authentication (for automatic team submission - optional)
# FPL_EMAIL=your_email@example.com
# FPL_PASSWORD=your_fpl_password
# Note: FPL API is unofficial and may require session cookies

# Logging
LOG_LEVEL=INFO

# MCP Server
MCP_ENABLED=false
